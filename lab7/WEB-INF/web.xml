<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
 http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

    <display-name>lab7</display-name>
    <servlet>
        <servlet-name>CccR</servlet-name>
        <servlet-class>CccR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CccS</servlet-name>
        <servlet-class>CccS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CccR</servlet-name>
        <url-pattern>/CccR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CccS</servlet-name>
        <url-pattern>/CccS</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>