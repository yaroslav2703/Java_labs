<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
 http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

    <display-name>lab6</display-name>
    <context-param>
        <param-name>URL1</param-name>
        <param-value>http://localhost:80/lab6/url1.html</param-value>
    </context-param>
    <context-param>
        <param-name>URL2</param-name>
        <param-value>http://localhost:80/lab6/Url2</param-value>
    </context-param>
    <servlet>
        <servlet-name>Urln</servlet-name>
        <servlet-class>Urln</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Url2</servlet-name>
        <servlet-class>Url2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Urln</servlet-name>
        <url-pattern>/Urln</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Url2</servlet-name>
        <url-pattern>/Url2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>